{
  "name": "otp-telegram-longcelot",
  "version": "0.1.3",
  "private": false,
  "type": "module",
  "scripts": {
    "dev": "pnpm run build && node dist/index.js",
    "dev:watch": "tsc -w & nodemon --watch dist --exec \"node dist/index.js\"",
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "test": "pnpm run build && node --test tests/*.test.mjs",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "example:embedded": "pnpm run build && node examples/embedded/app.mjs"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "examples",
    "migrations",
    "knexfile.cjs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vannseavlong/otp-longcelot.git"
  },
  "keywords": [
    "otp",
    "telegram",
    "authentication",
    "auth",
    "2fa"
  ],
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "rate-limiter-flexible": "^9.0.1",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.6",
    "telegraf": "^4.16.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.10.6",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "pnpm": {
    "allowedBuiltDependencies": [
      "argon2",
      "sqlite3"
    ]
  }
}
